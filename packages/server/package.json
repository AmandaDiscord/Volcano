{
    "name": "volcano-server",
    "version": "2.0.0-test-2",
    "description": "A light-weight LavaLink compatible replacement",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "compile": "tsup",
        "start": "node --enable-source-maps --optimize_for_size .",
        "create-release": "node ./scripts/release.js",
        "auth-gen": "node ./scripts/auth-gen.js",
        "dep-report": "node ./scripts/generateDependencyReport.js",
        "lint": "eslint . --ext .ts"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0",
        "yarn": ">=3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AmandaDiscord/Volcano.git"
    },
    "keywords": [
        "discord",
        "voice",
        "lavalink",
        "light-weight",
        "modular",
        "volcano"
    ],
    "author": "PapiOphidian",
    "license": "MIT",
    "dependencies": {
        "@lavalink/encoding": "^0.1.2",
        "@melike2d/songbird": "^1.0.0-rc.5",
        "backtracker": "3.3.2",
        "html-entities": "^2.3.3",
        "m3u8stream": "^0.8.6",
        "music-metadata": "^8.1.0",
        "node-html-parser": "^6.1.4",
        "play-dl": "^1.9.6",
        "twitch-m3u8": "github:AmandaDiscord/twitch-m3u8#ba927b26fb3c5cb118d75c20f990c5cce8a1ae7a",
        "volcano-sdk": "workspace:^",
        "ws": "^8.11.0",
        "yaml": "^2.1.3",
        "ytmusic-api": "^3.1.1"
    },
    "devDependencies": {
        "@types/node": "^18.11.9",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "eslint": "^8.27.0",
        "lavalink-types": "1.0.2",
        "tsup": "^6.5.0",
        "typescript": "^4.9.3"
    },
    "files": [
        "./dist",
        "./LICENSE",
        "./README.md"
    ]
}
