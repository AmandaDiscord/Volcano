{
  "name": "volcano",
  "version": "1.5.4",
  "description": "A light-weight LavaLink compatible replacement",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "compile": "tsc -p .",
    "start": "node --enable-source-maps --optimize_for_size .",
    "create-release": "node ./scripts/release.js",
    "auth-gen": "node ./scripts/auth-gen.js",
    "dep-report": "node ./scripts/generateDependencyReport.js",
    "lint": "eslint . --ext .ts"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmandaDiscord/Volcano.git"
  },
  "keywords": [
    "discord",
    "voice",
    "lavalink",
    "light-weight",
    "modular",
    "volcano"
  ],
  "author": "PapiOphidian",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "@lavalink/encoding": "^0.1.2",
    "backtracker": "^3.3.1",
    "ffmpeg-static": "^5.1.0",
    "html-entities": "^2.3.3",
    "m3u8stream": "^0.8.6",
    "music-metadata": "^8.1.0",
    "node-html-parser": "^6.1.1",
    "play-dl": "^1.9.6",
    "prism-media": "^1.3.4",
    "sodium-native": "^3.4.1",
    "twitch-m3u8": "github:AmandaDiscord/twitch-m3u8#ba927b26fb3c5cb118d75c20f990c5cce8a1ae7a",
    "volcano-sdk": "github:AmandaDiscord/VolcanoSDK#c9195e6891ec641ec9a9da3e7cb87a47fa6f8033",
    "ws": "^8.11.0",
    "yaml": "^2.1.3",
    "ytmusic-api": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "lavalink-types": "github:AmandaDiscord/LavalinkTypes#1cadb97f9b52ec93f4023cd1275000c31db81901",
    "typescript": "^4.8.4"
  },
  "files": [
    "./dist",
    "./LICENSE",
    "./README.md"
